        "titleBar.activeForeground": "#CCCCCC",
        "titleBar.border": "#2B2B2B",
        "titleBar.inactiveBackground": "#1F1F1F",
        "titleBar.inactiveForeground": "#9D9D9D",
        "welcomePage.tileBackground": "#2B2B2B",
        "welcomePage.progress.foreground": "#0078D4",
        "widget.border": "#313131",
        "editorBracketHighlight.foreground1": "#b294bb",
        "editorBracketHighlight.foreground2": "#8abeb7",
        "editorBracketHighlight.foreground3": "#f0c674",
        "terminalCursor.foreground": "#f5c2e7",
        "terminal.ansiBlack": "#181818",
        "terminal.ansiBlue": "#81a2be",
        "terminal.ansiCyan": "#8abeb7",
        "terminal.ansiGreen": "#b5bd68",
        "terminal.ansiMagenta": "#b294bb",
        "terminal.ansiRed": "#cc6666",
        "terminal.ansiWhite": "#CCCCCC",
        "terminal.ansiYellow": "#f0c674",
        "terminal.ansiBrightBlack": "#1F1F1F",
        "terminal.ansiBrightBlue": "#7aa6da",
        "terminal.ansiBrightCyan": "#70c0b1",
        "terminal.ansiBrightGreen": "#b9ca4a",
        "terminal.ansiBrightMagenta": "#c397d8",
        "terminal.ansiBrightRed": "#d54e53",
        "terminal.ansiBrightWhite": "#eaeaea",
        "terminal.ansiBrightYellow": "#e7c547",
    },
    "tokenColors": [
        {
            "scope": "header",
            "settings": {
                "foreground": "#b294bb"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#cc6666",
                "fontStyle": "italic underline"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#888888",
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#de935f"
            }
        },
        {
            "name": "Language tokens",
            "scope": [
                "entity.name.tag",
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#b294bb"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#CCCCCC"
            }
        },
        {
            "name": "Primitive values",
            "scope": [
                "constant.character",
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#81a2be"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#b5bd68"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#de935f"
            }
        },
        {
            "name": "Classes & namespaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "entity.other.inherited-class",
                "support.class",
                "support.type",
                "support.other.namespace"
            ],
            "settings": {
                "foreground": "#8abeb7"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#f0c674",
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "foreground": "#b294bb"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.strikethrough",
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b5bd68"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#cc6666"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#81a2be"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#f0c674"
            }
        },
    ]
}
