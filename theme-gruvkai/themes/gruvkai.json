        "editor.foreground": "#ebdbb2",
        "editor.lineHighlightBorder": "#32302f",
        "editorCursor.foreground": "#f5c2e7",
        "editorGroup.border": "#eaeaea17",
        "editorGroupHeader.tabsBackground": "#1d2021",
        "editorGroupHeader.tabsBorder": "#32302f",
        "editorGutter.addedBackground": "#2EA043",
        "editorGutter.deletedBackground": "#F85149",
        "editorGutter.modifiedBackground": "#0078D4",
        "editorLineNumber.activeForeground": "#ebdbb2",
        "editorLineNumber.foreground": "#928374",
        "editorOverviewRuler.border": "#010409",
        "editorWidget.background": "#1d2021",
        "errorForeground": "#F85149",
        "focusBorder": "#0078D4",
        "foreground": "#ebdbb2",
        "icon.foreground": "#ebdbb2",
        "input.background": "#32302f",
