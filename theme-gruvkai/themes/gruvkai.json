        "tab.activeBorder": "#282828",
        "tab.activeBorderTop": "#0078D4",
        "tab.activeForeground": "#eaeaea",
        "tab.border": "#1d2021",
        "tab.hoverBackground": "#282828",
        "tab.inactiveBackground": "#1d2021",
        "tab.inactiveForeground": "#a89984",
        "tab.unfocusedActiveBorder": "#282828",
        "tab.unfocusedActiveBorderTop": "#32302f",
        "tab.unfocusedHoverBackground": "#282828",
        "terminal.tab.activeBorder": "#0078D4",
        "textBlockQuote.background": "#32302f",
        "textBlockQuote.border": "#616161",
        "textCodeBlock.background": "#32302f",
        "textLink.activeForeground": "#4daafc",
        "textLink.foreground": "#4daafc",
        "textPreformat.foreground": "#D0D0D0",
        "textPreformat.background": "#3C3C3C",
        "textSeparator.foreground": "#21262D",
        "titleBar.activeBackground": "#1d2021",
        "titleBar.activeForeground": "#ebdbb2",
        "titleBar.border": "#32302f",
        "titleBar.inactiveBackground": "#282828",
        "titleBar.inactiveForeground": "#a89984",
        "welcomePage.tileBackground": "#32302f",
        "welcomePage.progress.foreground": "#0078D4",
        "widget.border": "#32302f",
        "editorBracketHighlight.foreground1": "#ebdbb2",
        "editorBracketHighlight.foreground2": "#83a598",
        "editorBracketHighlight.foreground3": "#b8bb26",
        "terminalCursor.foreground": "#f5c2e7",
        "terminal.foreground": "#ebdbb2",
        "terminal.ansiBlack": "#282828",
        "terminal.ansiBlue": "#458588",
        "terminal.ansiCyan": "#689d6a",
        "terminal.ansiGreen": "#98971a",
        "terminal.ansiMagenta": "#b16286",
        "terminal.ansiRed": "#cc241d",
        "terminal.ansiWhite": "#a89984",
        "terminal.ansiYellow": "#d79921",
        "terminal.ansiBrightBlack": "#928374",
        "terminal.ansiBrightBlue": "#83a598",
        "terminal.ansiBrightCyan": "#8ec07c",
        "terminal.ansiBrightGreen": "#b8bb26",
        "terminal.ansiBrightMagenta": "#d3869b",
        "terminal.ansiBrightRed": "#fb4934",
        "terminal.ansiBrightWhite": "#ebdbb2",
        "terminal.ansiBrightYellow": "#fabd2f",
    },
    "tokenColors": [
        {
            "scope": "header",
            "settings": {
                "foreground": "#fe8019"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#fb4934",
                "fontStyle": "italic underline"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#fb4934"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "markup.error",
            "settings": {
                "foreground": "#fb4934",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#fe8019"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "name": "Language tokens",
            "scope": [
                "entity.name.tag",
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#fe8019"
            }
        },
        {
            "name": "Variables",
            "scope": "variable",
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "name": "Primitive values",
            "scope": [
                "constant.character",
                "constant.numeric",
                "constant.language",
                "variable.other.constant",
                "support.constant"
            ],
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "name": "Regular expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "name": "Comments",
            "scope": "comment",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "name": "Classes & namespaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "entity.other.inherited-class",
                "support.class",
                "support.type",
                "support.other.namespace"
            ],
            "settings": {
                "foreground": "#83a598"
            }
        },
        {
            "name": "Functions",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#b8bb26",
            }
        },
        {
            "name": "Function parameters",
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
    ],
}
