{
    "$schema": "vscode://schemas/color-theme",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        "activityBar.activeBorder": "#0078D4",
        "activityBar.background": "#1d2021",
        "activityBar.inactiveForeground": "#888888",
        "sideBar.background": "#1d2021",
        "sideBarSectionHeader.background": "#282828",
        "editor.background": "#282828",
        "editor.foreground": "#ebdbb2",
        "editor.findMatchBackground": "#9E6A03",
        "editor.lineHighlightBackground": "#303030",
        "editorCursor.foreground": "#f5c2e7",
        "editorGroupHeader.tabsBackground": "#1d2021",
        "editorWidget.background": "#1d2021",
        "editorHoverWidget.background": "#1d2021",
        "editorSuggestWidget.background": "#1d2021",
        "editorSuggestWidget.foreground": "#CCCCCC",
        "debugToolBar.background": "#1d2021",
        "tab.activeBorderTop": "#0078D4",
        "tab.border": "#1d2021",
        "tab.hoverBackground": "#282828",
        "tab.inactiveBackground": "#1d2021",
        "panelTitle.activeBorder": "#0078D4",
        "panel.background": "#282828",
        "statusBar.background": "#1d2021",
        "statusBar.foreground": "#C0C0C0",
        "statusBar.noFolderBackground": "#1d2021",
        "statusBar.focusBorder": "#0078D4",
        "statusBarItem.focusBorder": "#0078D4",
        "titleBar.activeBackground": "#1d2021",
        "peekViewEditor.background": "#282828",
        "peekViewResult.background": "#282828",
        "notifications.background": "#1d2021",
        "editorBracketHighlight.foreground1": "#ebdbb2",
        "editorBracketHighlight.foreground2": "#83a598",
        "editorBracketHighlight.foreground3": "#b8bb26",
        "terminalCursor.foreground": "#f5c2e7",
        "terminal.foreground": "#ebdbb2",
        "terminal.ansiBlack": "#282828",
        "terminal.ansiBlue": "#458588",
        "terminal.ansiCyan": "#689d6a",
        "terminal.ansiGreen": "#98971a",
        "terminal.ansiMagenta": "#b16286",
        "terminal.ansiRed": "#cc241d",
        "terminal.ansiWhite": "#a89984",
        "terminal.ansiYellow": "#d79921",
        "terminal.ansiBrightBlack": "#928374",
        "terminal.ansiBrightBlue": "#83a598",
        "terminal.ansiBrightCyan": "#8ec07c",
        "terminal.ansiBrightGreen": "#b8bb26",
        "terminal.ansiBrightMagenta": "#d3869b",
        "terminal.ansiBrightRed": "#fb4934",
        "terminal.ansiBrightWhite": "#ebdbb2",
        "terminal.ansiBrightYellow": "#fabd2f",
    },
    "tokenColors": [
        {
            "scope": "header",
            "settings": {
                "foreground": "#fe8019"
            }
        },
        {
            "scope": "invalid",
            "settings": {
                "foreground": "#fb4934",
                "fontStyle": "italic underline"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#fb4934"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "markup.error",
            "settings": {
                "foreground": "#fb4934",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#fe8019"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "name": "Language tokens",
            "scope": [
                "entity.name.tag",
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#fe8019"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#ebdbb2"
            }
        },
        {
            "name": "Primitive values",
            "scope": [
                "constant.character",
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#d3869b"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#fabd2f"
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#8ec07c"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#928374"
            }
        },
        {
            "name": "Classes & namespaces",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "entity.other.inherited-class",
                "support.class",
                "support.type",
                "support.other.namespace"
            ],
            "settings": {
                "foreground": "#83a598"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#b8bb26",
            }
        },
        {
            "scope": "variable.parameter",
            "settings": {
                "fontStyle": "italic"
            }
        },
    ],
}
