        "notificationCenterHeader.foreground": "#ebdbb2",
        "notifications.background": "#282828",
        "notifications.border": "#32302f",
        "notifications.foreground": "#ebdbb2",
        "panel.background": "#1d2021",
        "panel.border": "#32302f",
        "panelInput.border": "#32302f",
        "panelTitle.activeBorder": "#0078D4",
        "panelTitle.activeForeground": "#ebdbb2",
        "panelTitle.inactiveForeground": "#a89984",
        "peekViewEditor.background": "#282828",
        "peekViewEditor.matchHighlightBackground": "#BB800966",
        "peekViewResult.background": "#282828",
        "peekViewResult.matchHighlightBackground": "#BB800966",
        "pickerGroup.border": "#3C3C3C",
        "progressBar.background": "#0078D4",
        "quickInput.background": "#222222",
        "quickInput.foreground": "#ebdbb2",
        "settings.dropdownBackground": "#32302f",
        "settings.dropdownBorder": "#3C3C3C",
        "settings.headerForeground": "#eaeaea",
        "settings.modifiedItemIndicator": "#BB800966",
        "sideBar.background": "#1d2021",
        "sideBar.border": "#32302f",
        "sideBar.foreground": "#ebdbb2",
        "sideBarSectionHeader.background": "#1d2021",
        "sideBarSectionHeader.border": "#32302f",
        "sideBarSectionHeader.foreground": "#ebdbb2",
        "sideBarTitle.foreground": "#ebdbb2",
        "statusBar.background": "#1d2021",
        "statusBar.border": "#32302f",
        "statusBar.debuggingBackground": "#0078D4",
        "statusBar.debuggingForeground": "#eaeaea",
        "statusBar.focusBorder": "#0078D4",
        "statusBar.foreground": "#ebdbb2",
        "statusBar.noFolderBackground": "#282828",
        "statusBarItem.focusBorder": "#0078D4",
        "statusBarItem.prominentBackground": "#6E768166",
        "statusBarItem.remoteBackground": "#0078D4",
        "statusBarItem.remoteForeground": "#eaeaea",
