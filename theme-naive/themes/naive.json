{
    "$schema": "vscode://schemas/color-theme",
    "name": "Naive",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "class": "#e5c07b",
        "comment": "#969896",
        "function": "#c397d8",
        "keyword": "#51afef",
        "namespace": "#e5c07b",
        "number": "#da8548",
        "parameter": {
            "fontStyle": "italic"
        },
        "regexp": "#75d5c1",
        "string": "#98c379",
        "variable": "#d4d4d4",
    },
    "colors": {
        "activityBar.activeBorder": "#0078d4",
        "activityBar.background": "#2c2c30",
        "editor.background": "#2c2c30",
        "editor.findMatchBackground": "#9e6a03",
        "editor.foreground": "#d4d4d4",
        "editor.lineHighlightBorder": "#37373c",
        "editorBracketHighlight.foreground1": "#0099ff",
        "editorBracketHighlight.foreground2": "#fec459",
        "editorBracketHighlight.foreground3": "#c678dd",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ec3e45",
        "editorCursor.foreground": "#f5bde6",
        "editorGroupHeader.tabsBackground": "#252526",
        "editorGroupHeader.tabsBorder": "#2c2c30",
        "panel.background": "#2c2c30",
        "panelTitle.activeBorder": "#0078d4",
        "peekViewEditor.background": "#2c2c30",
        "peekViewResult.background": "#2c2c30",
        "sideBar.background": "#252526",
        "sideBarSectionHeader.background": "#2c2c30",
        "statusBar.background": "#37373c",
        "statusBar.foreground": "#eaeaea",
        "statusBar.debuggingBackground": "#37373c",
        "statusBar.debuggingForeground": "#eaeaea",
        "statusBar.noFolderBackground": "#37373c",
        "statusBar.noFolderForeground": "#eaeaea",
        "tab.activeBackground": "#2c2c30",
        "tab.activeBorderTop": "#0078d4",
        "tab.border": "#252526",
        "tab.hoverBackground": "#2c2c30",
        "tab.inactiveBackground": "#252526",
        "tab.unfocusedActiveBorderTop": "#2c2c30",
        "tab.unfocusedHoverBackground": "#2c2c30",
        "terminalCursor.foreground": "#f5bde6",
        "terminal.ansiBlack": "#252526",
        "terminal.ansiBlue": "#51afef",
        "terminal.ansiCyan": "#75d5c1",
        "terminal.ansiGreen": "#98c379",
        "terminal.ansiMagenta": "#c397d8",
        "terminal.ansiRed": "#ff6c6b",
        "terminal.ansiWhite": "#d4d4d4",
        "terminal.ansiYellow": "#e5c07b",
        "terminal.ansiBrightBlack": "#969896",
        "terminal.ansiBrightBlue": "#0099ff",
        "terminal.ansiBrightCyan": "#32d6b3",
        "terminal.ansiBrightGreen": "#73ca33",
        "terminal.ansiBrightMagenta": "#c678dd",
        "terminal.ansiBrightRed": "#ec3e45",
        "terminal.ansiBrightWhite": "#eaeaea",
        "terminal.ansiBrightYellow": "#fec459",
    },
    "tokenColors": [
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#51afef"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#ec3e45",
                "fontStyle": "underline italic"
            }
        },
        {
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#969896",
                "fontStyle": "strikethrough"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#969896"
            }
        },
        {
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#c678dd"
            }
        },
        {
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#969896"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#fec459"
            }
        },
        {
            "scope": [
                "markup.error"
            ],
            "settings": {
                "foreground": "#ec3e45"
            }
        },
        {
            "name": "Underlined markup",
            "scope": [
                "markup.underline"
            ],
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Bold markup",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "name": "Markup headings",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0099ff"
            }
        },
        {
            "name": "Markup italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Inline code (prose)",
            "scope": [
                "markup.inline.raw",
                "markup.raw.restructuredtext"
            ],
            "settings": {
                "foreground": "#ff8b43"
            }
        },
        {
            "name": "Blockquotes (prose)",
            "scope": [
                "entity.name.directive.restructuredtext",
                "markup.quote"
            ],
            "settings": {
                "foreground": "#73ca33"
            }
        },
        {
            "name": "Horizontal rule (prose)",
            "scope": [
                "meta.separator.markdown"
            ],
            "settings": {
                "foreground": "#32d6b3"
            }
        },
        {
            "name": "Code blocks",
            "scope": [
                "fenced_code.block.language",
                "markup.raw.inner.restructuredtext",
                "markup.fenced_code.block.markdown punctuation.definition.markdown"
            ],
            "settings": {
                "foreground": "#ff8b43"
            }
        },
        {
            "name": "Language tokens",
            "scope": [
                "entity.name.tag",
                "keyword",
                "storage",
                "storage.type.class",
                "storage.type.enum",
                "storage.type.function",
                "storage.type.interface",
                "storage.type.internaldeclaration",
                "storage.type.modifier",
                "storage.type.namespace",
                "storage.type.property",
                "storage.type.trait",
                "storage.type.type"
            ],
            "settings": {
                "foreground": "#51afef"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#d4d4d4"
            }
        },
        {
            "name": "Primitive values",
            "scope": [
                "constant.character",
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#da8548"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "Regular expressions",
            "scope": [
                "string.regexp",
                "string.regexp punctuation.definition"
            ],
            "settings": {
                "foreground": "#75d5c1"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "unused.comment",
                "wildcard.comment"
            ],
            "settings": {
                "foreground": "#969896"
            }
        },
        {
            "name": "Classes & namespaces",
            "scope": [
                "entity.name.type",
                "entity.name.namespace",
                "entity.other.inherited-class",
                "support.class",
                "support.other.namespace",
                "support.type"
            ],
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "entity.other.attribute-name",
                "support.function"
            ],
            "settings": {
                "foreground": "#c397d8",
                "fontStyle": ""
            }
        },
        {
            "name": "Function parameters",
            "scope": [
                "entity.name.variable.parameter",
                "variable.parameter"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
    ],
}
